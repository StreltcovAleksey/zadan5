begin
ListBox1.Items.Add('Поступил звонок на '+ Label4.Caption + '. ' + 'Этому звонку присвоен ID= '+ Edit4.Text);
ListBox1.Items.add('Звонок номер '+ edit4.Text+ ' с телефона ' + label4.Caption + ' обработан и передан в базу');
